{
  "name": "node-koa-mongodb",
  "version": "1.0.0",
  "description": "node-koa-mongodb 搭建node服务端",
  "main": "src/index.ts",
  "scripts": {
    "dev": "npm run dev-gulp",
    "dev-gulp": "gulp start",
    "dev-node": "nodemon --exec 'ts-node --project tsconfig.json' ./src/index.ts",
    "build": "gulp build",
    "watch": "npm run tslint && npm run watch-ts && npm run tslint",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "keywords": [
    "node",
    "koa",
    "mongodb",
    "服务端"
  ],
  "author": "crassland_wolf[511409899@qq.com]",
  "license": "ISC",
  "devDependencies": {
    "@types/koa": "^2.0.46",
    "nodemon": "^1.17.5",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-sequence": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^5.0.0-alpha.1",
    "http": "^0.0.0",
    "https": "^1.0.0",
    "koa": "^2.5.1"
  }
}
